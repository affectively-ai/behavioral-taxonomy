{
    "name": "@affectively/behavioral-taxonomy",
    "version": "1.0.0",
    "description": "A structured taxonomy of 1,140+ human behavioral loops, emotions, cognitive biases, and personality traits for affective computing applications.",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./data/behavioral-loops": {
            "import": "./data/behavioralLoops.json",
            "require": "./data/behavioralLoops.json"
        },
        "./data/emotions": {
            "import": "./data/emotions.json",
            "require": "./data/emotions.json"
        },
        "./data/biases": {
            "import": "./data/biases.json",
            "require": "./data/biases.json"
        },
        "./data/traits": {
            "import": "./data/traits.json",
            "require": "./data/traits.json"
        }
    },
    "files": [
        "dist",
        "data",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "prepublishOnly": "npm run build",
        "test": "bun test",
        "lint": "eslint src/"
    },
    "keywords": [
        "behavioral-science",
        "emotions",
        "psychology",
        "affective-computing",
        "cognitive-biases",
        "personality-traits",
        "mental-health",
        "emotion-ai",
        "nlp",
        "taxonomy"
    ],
    "author": "AFFECTIVELY <opensource@affectively.app>",
    "license": "CC-BY-4.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/affectively-ai/behavioral-taxonomy.git"
    },
    "bugs": {
        "url": "https://github.com/affectively-ai/behavioral-taxonomy/issues"
    },
    "homepage": "https://github.com/affectively-ai/behavioral-taxonomy#readme",
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.9.0"
    },
    "publishConfig": {
        "access": "public"
    }
}